{
  "manifest_version": 3,
  "name": "AutoDelHis",
  "version": "1.0",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "action": {
    "default_icon": "images/icon128.png",
    "default_popup": "popup.html"
  },
  "permissions": ["history","storage"],
  "background": {
    "service_worker": "background.js"
  }
}
